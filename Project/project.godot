[application]

config/name="Yet Another Factory Game"
run/main_scene="res://scene/TitleScreen.tscn"
boot_splash/image="res://textures/misc/preload.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

BaseFuncs="*res://gdstuff/Scripts/basefunctions.gd"
Discord="*res://addons/discord_game_sdk/discord.gd"
DiscordRpc="*res://gdstuff/discord_rpc.gd"

[debug]

settings/crash_handler/message="Report this error to DoesStuff#5723 on discord/@wtpts on twitter"

[editor_plugins]

enabled=PoolStringArray( "res://addons/discord_game_sdk/plugin.cfg", "res://addons/json_editor/plugin.cfg" )

[global]

Version="2022-05.31"
ReleaseNumber="0.1_4"
Branch="Dev"
Changelogs=PoolStringArray( "[b]Version 0.1_4 (DevSrc):[/b]\\n\\nVersion Definitions:\\nU: Unreleased\\nD: Demo\\nDX: Development\\nE: Early\\nrelease versions will have no symbol, nightly versions will have the E symbol.\\n\\nBranches:\\nDev: Development Branch\\nNightly: Nightly Branch\\nMain/Release: Stable Branch\\n\\n[b]Changes[/b]\\n- Now using the list properly\\n- Status now shows the game\'s version\\n[b]Additions[/b]\\n- 3 new discord RPC icons", "[b]Version 0.1_3 (DevUnrel):[/b]\\n[b]Changes[/b]\\n- Title Screen has a dark background by default\\n- Movement is now handled by a base function using tweens, making speed the same for everyone\\n- Setting icons inside the game are now ordered in 1 row\\n- Conveyor\'s file is now Conveyor.tscn\\n- Using NinePatchRect and Panels instead of flat textures\\n- Versions are now formatted like the following:\\n-- Release Type, Release Number, Branch\\n[b]Removals[/b]\\n- Removed a lot of boilerplate from items\\n- Support for texture overrides is paused until further updates\\n[b]Additions[/b]\\n- Credits and Changelogs are now accesible on the title screen\\n" )
"Release Type"="E"

